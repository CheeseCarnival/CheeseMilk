<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="OFF">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5p] %d{yyyy-MM-dd HH:mm:ss,SSS} method:%l%n%m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <Appender-ref ref="Console"/>
        </Root>
    </Loggers>
</Configuration>